 flex;\r\n  align-items: center;\r\n  justify-content: center;\r\n  height: 4.8rem;\r\n  width: 9rem;\r\n  border: none;\r\n  font-family: \"Roboto\", sans-serif;\r\n  padding: 0.8rem;\r\n  border-radius: 9px;\r\n  font-weight: 500;\r\n  font-size: 1.6rem;\r\n  background-color: #1864ab;\r\n  color: #fff;\r\n  transition: all 0.3s;\r\n}\r\n.btn:hover {\r\n  background-color: #1971c2;\r\n  color: #e7f5ff;\r\n  box-shadow: 0 0 0 0.3rem rgba(24, 100, 171, 0.5);\r\n}\r\n\r\n.w-container {\r\n  display: flex;\r\n  flex-direction: column;\r\n  align-items: center;\r\n  justify-content: center;\r\n  width: 30rem;\r\n  height: 40rem;\r\n  background-color: rgba(255, 255, 255, 0.4);\r\n  border-radius: 9px;\r\n  padding: 2rem;\r\n}\r\n\r\n.loading {\r\n  display: flex;\r\n  justify-content: center;\r\n}\r\n\r\n.loading::after {\r\n  content: \"\";\r\n  width: 3rem;\r\n  height: 3rem;\r\n\r\n  border: 0.5rem solid #777;\r\n  border-top-color: #1864ab;\r\n  border-bottom-color: #1864ab;\r\n  border-radius: 50%;\r\n  animation: loading 1s linear infinite;\r\n}\r\n\r\n@keyframes loading {\r\n  to {\r\n    transform: rotate(1turn);\r\n  }\r\n}\r\n\r\n.country-info {\r\n  display: flex !important;\r\n  flex-direction: row;\r\n  gap: 0.2rem;\r\n  margin-bottom: 3rem;\r\n}\r\n\r\n.city-form {\r\n  font-size: 3rem;\r\n\r\n  transition: all 0.6s ease-in;\r\n}\r\n\r\n.country {\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: center;\r\n  width: 1.7rem;\r\n  height: 1.7rem;\r\n  padding: 0.1rem;\r\n\r\n  color: #fff;\r\n  background-color: #1864ab;\r\n  font-weight: 500;\r\n  border-radius: 2px;\r\n}\r\n.temp {\r\n  font-size: 6.4rem;\r\n  font-weight: 500;\r\n  margin-bottom: 5rem;\r\n}\r\n.icon {\r\n  font-size: 8rem;\r\n  color: #1864ab;\r\n  margin-bottom: 2.4rem;\r\n}\r\n\r\n.icon-img {\r\n  width: 13rem;\r\n  height: 13rem;\r\n  margin-bottom: 2.4rem;\r\n}\r\n.w-comment {\r\n  font-size: 2.4rem;\r\n}\r\n\r\n*:focus {\r\n  outline: none;\r\n  box-shadow: 0 0 0 0